<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.timeline.vpn"
    android:versionCode="26"
    android:versionName="1.4.5">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    <meta-data
        android:name="com.timeline.vpn.common.util.MyGlideModule"
        android:value="GlideModule" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <application
        android:name=".base.MyApplication"
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainFragment"
            android:label="@string/main_activity_name"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <service android:name=".service.CharonVpnService"
            android:exported="false"
            android:permission="android.permission.BIND_VPN_SERVICE">
            <!--android:process="com.timeline.vpn.service.vpnservice">-->
            <intent-filter>
                <action android:name="android.net.VpnService" />
            </intent-filter>
        </service>
    </application>

</manifest>
